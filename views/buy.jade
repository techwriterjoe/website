  body
    .container-fluid(style="padding: 0px;")
      div(data-wizard-init='')
        ul.steps
          li(data-step='1') Amount
          li(data-step='2') You
          li(data-step='3') Address
          li(data-step='4') Summary
          li(data-step='5') Payment
        .steps-content
          div(data-step='1')
            p(style="text-align: center")
              | fuck off
            .circle.c1.img-circle(style="margin-left:140px")
                 h4.blue Current Price
                 br
                 span.price-large.blue 8
                 span.price-small ,000
                 p ELC per 1 Bitcoin (413,13 EUR)


          div(data-step='2')
            p
              fieldset
                 // Text input
                 .form-group
                   label.col-md-4.control-label(for='textinput') Your Full Name:
                   .col-md-4
                     input#textinput.form-control.input-md(name='textinput', type='text', placeholder='Max Mustermann')
                     span.help-block Please tell us who you are, the invoice will be addresses to you
                 // Text input
                 .form-group
                   label.col-md-4.control-label(for='textinput') Your E-Mail Address:
                   .col-md-4
                     input#textinput.form-control.input-md(name='textinput', type='text', placeholder='max@mustermann.io', required='')
                     span.help-block
                       | You will receive the invoice as well as instructions how to redeem your coins to that address
                 // Multiple Checkboxes
                 .form-group
                   label.col-md-4.control-label(for='checkboxes') Before We Start:
                   .col-md-4
                     .checkbox
                       label(for='checkboxes-0')
                         input#checkboxes-0(type='checkbox', name='checkboxes', value='1')
                         |       You have read, fully understood and accepted our Terms of Agreement
                     .checkbox
                       label(for='checkboxes-1')
                         input#checkboxes-1(type='checkbox', name='checkboxes', value='2')
                         |       You accept that the satuatory right of withdrawal expires the moment something strange happens
          div(data-step='3')
            p
              fieldset
                 .control-group
                   label.control-label Your Address Line 1
                   .controls
                     input#address-line1.input-xlarge(name='address-line1', type='text', placeholder='address line 1')
                     p.help-block Street address, P.O. box, company name, c/o
                 // address-line2 input
                 .control-group
                   label.control-label Your Address Line 2
                   .controls
                     input#address-line2.input-xlarge(name='address-line2', type='text', placeholder='address line 2')
                     p.help-block Apartment, suite , unit, building, floor, etc.
                 // city input
                 .control-group
                   label.control-label City / Town
                   .controls
                     input#city.input-xlarge(name='city', type='text', placeholder='city')
                     p.help-block
                 // region input
                 .control-group
                   label.control-label State / Province / Region
                   .controls
                     input#region.input-xlarge(name='region', type='text', placeholder='state / province / region')
                     p.help-block
                 // postal-code input
                 .control-group
                   label.control-label Zip / Postal Code
